
age = int(input())

if '7' in str(age):
    print('Вам повезло!')

else:
    if 0 < age < 6:
        print('Где твои родители?')

    elif 6 <= age < 16:
        print('Это фильм для взрослых!')

    elif 65 < age < 100
        print('Покажите пенсионное удостоверение!')

    else:
        print('А билетов уже нет!')